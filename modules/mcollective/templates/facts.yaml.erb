<%=
    obj = scope.to_hash.reject { |k,v| k.to_s =~ /^(uptime.*|rubysitedir|_timestamp|memoryfree.*|swapfree.*|title|name|path|caller_module_name|module_name|sshfp.*|mc_passwd|sshdsakey|sshrsakey|taskid)$/ }
    out = "---\n"
    obj.keys.sort.each do |key|
        if obj[key].is_a?(String)
            out += "#{key}: #{obj[key]}\n"
        end
    end
    
    out
%>

